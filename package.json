{
  "name": "@gravity-ui/page-constructor-builder",
  "version": "1.0.0",
  "description": "A command line utility for building static pages from YAML configurations using @gravity-ui/page-constructor",
  "main": "dist/index.js",
  "bin": {
    "page-builder": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "npm run lint && tsc --noEmit && jest",
    "test:unit": "jest",
    "clean": "rimraf dist",
    "build:example": "npm run build && rm -rf example/dist && cd example && node ../dist/cli.js build && open dist/index.html",
    "build:example:watch": "npm run build && rm -rf example/dist && cd example && node ../dist/cli.js build --watch",
    "prepack": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "page-constructor",
    "ssg",
    "gravity-ui",
    "cli",
    "typescript"
  ],
  "author": "pistch@yandex-team.ru",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@diplodoc/transform": "^4.0.0",
    "@gravity-ui/page-constructor": "^7.5.0",
    "@gravity-ui/uikit": "^7.1.1",
    "babel-loader": "^9.1.0",
    "buffer": "^6.0.3",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.0",
    "commander": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.8.0",
    "fs-extra": "^11.0.0",
    "glob": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "null-loader": "^4.0.1",
    "ora": "^7.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.63.0",
    "sass-loader": "^13.3.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@gravity-ui/eslint-config": "^3.0.0",
    "@gravity-ui/prettier-config": "^1.0.0",
    "@gravity-ui/stylelint-config": "^4.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "husky": "^9.1.7",
    "jest": "^29.6.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "stylelint": "^15.10.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
